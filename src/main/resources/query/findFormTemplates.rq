prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix fm: <http://onto.fel.cvut.cz/ontologies/form-metadata/>
prefix dcterms: <http://purl.org/dc/terms/>

CONSTRUCT {
      ?formGraph rdfs:label ?label .
      ?formGraph rdfs:comment ?explanation .
}
WHERE {
    GRAPH ?formGraph {
       ?formRoot a fm:root-entity .
       OPTIONAL {
            ?formGraph skos:prefLabel ?formLabel .
       }
    }
    SERVICE <https://onto.fel.cvut.cz:7200/repositories/kodi-slovnik-gov-cz> {
      ?formRoot skos:prefLabel ?entityLabel .
      OPTIONAL {
         ?formRoot dcterms:description ?explanation .
      }
   }
   BIND(COALESCE(?formLabel, ?entityLabel) as ?label)
}